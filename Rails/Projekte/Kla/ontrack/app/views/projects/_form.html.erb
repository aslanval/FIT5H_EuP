<%= form_for(@project) do |f| %>
<%= render "shared/errors", objekt_errors: @project %>
	<div class="row">
		<div class="medium-2 columns">
			<%= f.label :title %>
		</div>
		<div class="medium-10 columns">
			<%= f.text_field :title %>
		</div>
	</div>
	<div class="row">
		<div class="medium-2 columns">
			<%= f.label :description %>
		</div>
		<div class="medium-10 columns">
			<%= f.text_field :description %>
		</div>
	</div>
	<div class="row">
		<div class="medium-2 columns">
			<%= f.label :start_date %>
		</div>
		<div class="medium-10 columns">
			<%= f.datetime_select :start_date %>
		</div>
	</div>
	<div class="row">
		<div class="medium-2 columns">
			<%= f.submit %>
		</div>
		<div class="medium-10 columns">
			<%= link_to "Back", projects_path %>
		</div>
	</div>
<% end %>